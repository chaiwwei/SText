.button { 
  display: inline-block; 
  padding-top: 5px;
  padding-bottom: 5px; 
  margin-top: 15px;
  font-size: 16px; 
  cursor: pointer; 
  text-align: center; 
  text-decoration: none; 
  outline: none; 
  color: #000; 
  background-color: #99CCFF; 
  border-radius: 5px; 
  width: 100%;
}
.button:hover {
  background-color: #3e8e41
} 
.button:active { 
  background-color: #3e8e41; 
  box-shadow: 0 5px #666; 
  transform: translateY(4px);
} 

select{
  width:90px;
}

