html, body {
  color:black;
  height: 100%;
}

li{
  list-style: none;
}

#editor{
	/* border: 2px solid; */
	width: 100%;
	height:100vh;
}

path.link {
  fill: none;
  stroke: #000;
  stroke-width: 4px;
  cursor: default;
}

path.link.selected {
  stroke-dasharray: 10,2;
}

path.link.dragline {
  pointer-events: none;
}

path.link.hidden {
  stroke-width: 0;
}

#chart{
  border:1px solid #2196F3;
  border-radius: 15px;
	position: absolute;
	width: 600px;
  height:530px;
  z-index: 10000;
  /* 编辑器有个控件z-index=4 所以要大于这个值才能显示在编辑器上米娜 */
}

#editorRow{
  z-index: 1;
}

#header{
  border-top-left-radius: 15px;
  border-top-right-radius: 15px;
  padding: 5px;
  cursor: move;
  background-color: #2196F3;
  color: #fff;
  height:30px;
  width: 600px;
  z-index: 6;
}

#content{
	overflow-x: hidden;
	overflow-y: scroll;
	width: 600px;
	height:300px;
}

svg {
  background-color: #FFF;
  cursor: default;
  width: 600px;
  height: 400px;
}

text {
  font: 24px Arial;
}

#footer{
  border-bottom-left-radius: 15px;
  border-bottom-right-radius: 15px;
	width: 600px;
	height: 200px;
	background-color: #FFF;
}

#footer label {
	text-align:right;
}​

#footer input{
  vertical-align: middle;
  margin: 5px 10px 5px 0;
  padding: 10px;
  background-color: #fff;
  border: 1px solid #ddd;
}

input{
  border-radius: 2 px;
  border:1px solid #2196F3;
  margin-top: 1px;
  margin-bottom: 1px;
  width: 100%;
}

textarea{
  border-radius: 10px;
  border:1px solid #2196F3;
  width: 100%;
}

.box{
  border:1px solid #2196F3;
  /* border-top: 1px outset;
  border-bottom: 1px outset; */
  background: #ffffff;

}

